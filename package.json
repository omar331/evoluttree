{
  "name": "evoluttree",
  "version": "0.2.4-c",
  "description": "A ReactJs based, drag and drop, tree-structured content management system.",
  "repository": {
    "type": "git",
    "url": "https://github.com/omar331/evoluttree.git"
  },
  "scripts": {
    "start": "webpack-dev-server --watch --inline --hot --TARGET=DEV",
    "test": "mocha --colors './src/tests/**/*.spec.ts' --compilers ts:ts-node/register",
    "test:watch": "watch-run -i -p './src/**' npm run test",
    "postinstall": "typings install",
    "deploy:development": "./node_modules/shipit-cli/bin/shipit development deploy",
    "deploy:staging": "./node_modules/shipit-cli/bin/shipit staging deploy",
    "deploy:production": "./node_modules/shipit-cli/bin/shipit production deploy",
    "prepublish": "npm run make:dist",
    "make:dist": "webpack --TARGET=DIST"
  },
  "keywords": [
    "cms",
    "content management",
    "react",
    "tree",
    "drag and drop",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "node-uuid": "^1.4.7",
    "react": "^15.2.0",
    "react-bootstrap": "^0.30.3",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.23.1",
    "expect": "^1.20.2",
    "minimist": "1.2.0",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "shipit": "1.0.2",
    "shipit-cli": "1.4.1",
    "shipit-deploy": "2.2.0",
    "style-loader": "^0.13.1",
    "superagent-mocker": "0.4.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.2.2",
    "tslint": "^3.13.0",
    "typescript": "^1.8.10",
    "typescript-require": "^0.2.9-1",
    "watch-run": "^1.2.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
